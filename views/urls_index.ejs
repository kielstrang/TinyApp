<!DOCTYPE html>
<html lang="en">

<head>
  <title>TinyApp</title>
</head>

<body>
  <% include partials/_header %>
  <table>
    <% for (const shortURL in urls) { %>
      <tr></tr>
        <form method="POST" action=<%= `/urls/${shortURL}/delete` %>>
          <td><a href=<%= `/urls/${shortURL}` %>><%= shortURL %></a> &rarr; </td>
          <td><a href=<%= `/u/${shortURL}` %>><%= urls[shortURL] %></a></td>
          <td><input type="submit" value="Delete"></td>
        </form>
      </tr>
      <% } %>
  </table>
  <br>
  <a href="/urls/new">Shorten New URL</a>
</body>

</html>